{
  "name": "kk_employee_portal_frontend",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Telesoftas",
    "email": "hello@telesoftas.com",
    "url": "https://telesoftas.com"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "tsc --noEmit && vue-cli-service lint",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/root/locales/*.json\"",
    "storybook": "start-storybook -p 6006",
    "exp:trans:node": "node ./bin/export-translations.js",
    "db": "json-server --host 192.168.31.7 data/db.json",
    "db-ofise": "json-server --host 192.168.20.238 data/db.json",
    "db-phone": "json-server --host 192.168.43.200 data/db.json"
  },
  "dependencies": {
    "@tailwindcss/line-clamp": "0.3.0",
    "@vueform/multiselect": "2.3.1",
    "@vueuse/core": "7.5.1",
    "npm-run-all": "4.1.5",
    "core-js": "3.20.1",
    "lodash": "4.17.21",
    "register-service-worker": "1.7.2",
    "vee-validate": "4.5.2",
    "vue": "3.2.19",
    "vue-i18n": "9.1.9",
    "vue-router": "4.0.11",
    "vue3-popper": "1.4.1",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@babel/core": "7.16.7",
    "@intlify/vue-i18n-loader": "4.1.0",
    "@types/google.maps": "3.47.2",
    "@types/jest": "27.4.0",
    "@types/lodash": "4.14.178",
    "@typescript-eslint/eslint-plugin": "5.8.1",
    "@typescript-eslint/parser": "5.8.1",
    "@vue/cli-plugin-babel": "5.0.0-beta.6",
    "@vue/cli-plugin-eslint": "5.0.0-beta.6",
    "@vue/cli-plugin-pwa": "5.0.0-beta.6",
    "@vue/cli-plugin-router": "5.0.0-beta.6",
    "@vue/cli-plugin-typescript": "5.0.0-beta.6",
    "@vue/cli-service": "5.0.0-beta.6",
    "@vue/compiler-sfc": "3.2.26",
    "@vue/eslint-config-airbnb": "5.3.0",
    "@vue/eslint-config-prettier": "7.0.0",
    "@vue/eslint-config-typescript": "10.0.0",
    "autoprefixer": "10.4.1",
    "babel-loader": "8.2.3",
    "dotenv-flow": "3.2.0",
    "eslint": "8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-vue": "8.2.0",
    "lint-staged": "12.1.4",
    "node-fetch": "2.6.1",
    "postcss": "8.4.5",
    "prettier": "2.5.1",
    "tailwindcss": "3.0.8",
    "ts-jest": "27.1.2",
    "typescript": "4.5.4",
    "vue-cli-plugin-i18n": "2.3.1",
    "vue-jest": "5.0.0-alpha.10",
    "vue-loader": "16.8.3",
    "vue-svg-loader": "0.17.0-beta.2",
    "wait-on": "6.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "cypress/globals": true
    },
    "extends": [
      "plugin:cypress/recommended",
      "@vue/airbnb",
      "plugin:vue/vue3-recommended",
      "@vue/eslint-config-typescript",
      "@vue/eslint-config-typescript/recommended",
      "@vue/prettier",
      "@vue/eslint-config-prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "parser": "@typescript-eslint/parser"
    },
    "rules": {
      "prettier/prettier": "error",
      "import/prefer-default-export": 0,
      "import/named": 0,
      "class-methods-use-this": 0,
      "no-shadow": "off",
      "@typescript-eslint/no-shadow": [
        "error"
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "browserslist": [
    "last 2 years",
    "not dead"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest/presets/typescript-and-babel",
    "transform": {
      "^.+\\.vue$": "vue-jest"
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
